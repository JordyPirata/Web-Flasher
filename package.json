{
  "name": "web-flasher",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "dist": "npm run css && npm run js",
    "dev:css": "npm run css -- --watch",
    "dev:js": "npm run js -- --watch",
    "dev:serve": "npm run serve",
    "dev": "npm-run-all --parallel dev:*",
    "css": "npx @tailwindcss/cli -i ./public/css/input.css -o ./public/css/styles.css",
    "js": "webpack --config webpack.config.js",
    "serve": "npx live-server public --wait=1000 --port=8081"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JordyPirata/Web-Flasher.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JordyPirata/Web-Flasher/issues"
  },
  "homepage": "https://github.com/JordyPirata/Web-Flasher#readme",
  "dependencies": {
    "@tailwindcss/cli": "^4.0.6",
    "android-fastboot": "^1.1.3",
    "lodash": "^4.17.21",
    "tailwindcss": "^4.0.6",
    "xzwasm": "^0.1.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
